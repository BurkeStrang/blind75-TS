{
    "name": "blind75-ts",
    "version": "1.0.0",
    "description": "data structure and algo practice",
    "scripts": {
        "test": "jest --watch",
        "prettier": "prettier --write .",
        "prettier-check": "prettier --check ."
    },
    "author": "",
    "license": "ISC",
    "type": "module",
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": [
                "ts-jest",
                {
                    "tsconfig": "tsconfig.json"
                }
            ]
        },
        "testMatch": [
            "**/test/**/*.test.(ts|tsx|js)"
        ]
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.12",
        "eslint": "^9.17.0",
        "globals": "^15.14.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.4.2",
        "typescript-eslint": "^8.18.2",
        "prettier": "^3.1.1",
        "@prettier/plugin-xml": "^3.2.2",
        "prettier-plugin-jsdoc": "^1.3.0",
        "prettier-plugin-multiline-arrays": "^3.0.1"
    }
}
